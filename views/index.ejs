            $(this).find($line).animate({'width': '100%'}, 'fast');
            $getWrapper.find($allTabs).hide(200);
            $getWrapper.find($allTabs).filter('[data-tab=' + dataTab + ']').show(200);
        });

    });//end ready
</script>
